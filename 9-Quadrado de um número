#include <stdio.h>
int square(int x) //Cálcula o quadrado de X
    {
        printf("O quadrado e %d",(x*x)); //Só se trabalha com 'x' dentro da square
    }

    int main()
    {
        int num;
        printf("Entre com um numero: ");
        scanf("%d", &num);
        printf("\n\n");
        square(num);
        return 0;
}
