#include <stdio.h>
int main(){
    float altura,largura,comprimento;
    printf("Voce tem uma caixa d'agua em formato de paralelepipedo\nInsira aqui a sua altura, em metros:");
    scanf("%f", &altura);
    printf("Agora, insira sua largura, em metros:");
    scanf("%f", &largura);
    printf("Por fim, insira seu comprimento, em metros:");
    scanf("%f", &comprimento);
    printf("Esse e o volume da sua caixa d'agua, em metros cubicos:%.2f", altura*largura*comprimento);
    return 0;
}
