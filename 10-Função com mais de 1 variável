#include <stdio.h>
int mult(float a, float b) //A função mult permite multiplicar quantos numeros você quiser, basta apenas acrescentar mais: "float _"
{
    printf("%f",a*b); //Esse "printf()" faz com que caso a função mult seja utilizada ela imprima o resultado da multiplicação SEM PRECISAR DE OUTRA printf() MAIS NA FRENTE
    return 0;
}
int main()
{
    float x,y;
    printf("Digite 2 numeros para serem multiplicados entre si: ");
    scanf("%f", &x);
    scanf("%f", &y);
    printf("\n\n");
    mult (x,y);
    return 0;
}
